{
  "name" :"pemedina/nordigen-api",
  "description" :"PHP Wrapper for Nordigen API.",
  "type" :"library",
  "license" :["MIT"],
  "keywords" :[
    "API",
    "PHP",
    "Nordigen",
    "OpenBanking"
  ],
  "autoload" :{
    "psr-4" :{
      "Pemedina\\Nordigen\\" :"src/"
    }
  },
  "autoload-dev" :{
    "psr-4" :{
      "Pemedina\\Nordigen\\Tests\\" :"tests/"
    }
  },
  "authors" :[
    {
      "name" :"Pablo E. Medina",
      "email" :"pablo.medina@fastmail.com"
    }
  ],
  "minimum-stability" :"dev",
  "prefer-stable" :true,
  "require" :{
    "php" :"^7.2 || ^8.0",
    "guzzlehttp/guzzle" :"^6.2.1|^7.0",
    "ext-json" :"*"
  },
  "require-dev" :{
    "phpunit/phpunit" :"^8.0 || ^9.4"
  }
}
